<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.a.fun_module">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".FunApp"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,MissingApplicationIcon">

        <activity android:name=".FunMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".customview.view.circleRefresh.CircleRefreshViewActivity" />
        <activity android:name=".customview.view.clock.ClockViewActivity" />
        <activity android:name=".customview.view.percentage.PercentageViewActivity" />
        <activity android:name=".customview.view.plan.BaseActivity" />
        <activity android:name=".customview.view.plan.PlanViewActivity" />
        <activity android:name=".customview.view.pointBeat.PointBeatViewActivity" />
        <activity android:name=".customview.view.rain.RainViewActivity" />
        <activity android:name=".customview.view.taiji.TaiJiViewActivity" />
        <activity android:name=".customview.view.wave.WaveViewActivity" />
        <activity android:name=".customview.view.waveLoading.WaveLoadingViewActivity" />
        <activity android:name=".scroll_view.FunScrollViewActivity" />
        <activity android:name=".custom_animation.CustomAnimationActivity" />
        <activity android:name=".custom_animation.MyCircleAnimationActivity" />

    </application>
</manifest>
